  "author": "katashin",
  "description": "A simple toast notification for Vuex",
  "keywords": [
    "UI",
    "Flux",
    "Vuex",
    "Vue",
    "toast"
  ],
  "license": "MIT",
  "main": "dist/vuex-toast.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/vuex-toast",
  "bugs": "https://github.com/ktsn/vuex-toast/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vuex-toast.git"
  },
  "scripts": {
    "prepublish": "npm run flow && npm run lint && npm run build",
    "build": "NODE_ENV=production gulp build",
    "build:example": "gulp build:example",
    "flow": "flow check",
    "lint": "eslint -c .eslintrc.yml \"src/**/*\" && eslint -c test/.eslintrc.yml \"test/**/*\"",
    "test": "gulp test",
    "test:ci": "webpack --config webpack.config.test.js && testem ci --launch Firefox,PhantomJS"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-html": "^1.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.27.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-header": "^1.7.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "js-yaml": "^3.6.0",
