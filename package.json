    "run-sequence": "^1.1.5",
    "testem": "^1.6.0",
    "vue": "^2.0.0-beta.1",
    "vue-loader": "^9.1.3",
    "vuex": "^2.0.0-rc.3",
    "webpack": "^1.13.0"
